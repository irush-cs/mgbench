<?xml version="1.0"?>
<PhoronixTestSuite>
    <TestInformation>
        <Title>GPU compute performance</Title>
        <AppVersion>1.0</AppVersion>
        <Description>GPU compute performance</Description>
        <ResultScale>ms</ResultScale>
        <Proportion>LIB</Proportion>
        <TimesToRun>1</TimesToRun>
    </TestInformation>
    <TestProfile>
        <Version>1.0.0</Version>
        <SupportedPlatforms>Linux</SupportedPlatforms>
        <SoftwareType>Benchmark</SoftwareType>
        <TestType>Other</TestType>
        <License>Free</License>
        <Status>Experimental</Status>
        <Extends>local/mgbench-base</Extends>
        <ExternalDependencies>cuda</ExternalDependencies>
        <ProjectURL>https://github.com/tbennun/mgbench</ProjectURL>
        <InternalTags>CUDA</InternalTags>
        <Maintainer>Tal Ben-Nun &amp; Yair Yarom</Maintainer>
    </TestProfile>
    <TestSettings>
        <Option>
            <DisplayName>Precision</DisplayName>
            <Identifier>precision</Identifier>
            <Menu>
                <Entry>
                    <Name>Matrix multiplication (singal)</Name>
                    <Value>single</Value>
                </Entry>
                <Entry>
                    <Name>Matrix multiplication (double)</Name>
                    <Value>double</Value>
                </Entry>
                <Entry>
                    <Name>Fixed point convolution</Name>
                    <Value>fixed</Value>
                </Entry>
            </Menu>
        </Option>
    </TestSettings>
</PhoronixTestSuite>
